<div class="modal fade" id="editModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header bg-warning">
                <h4 class="modal-title text-white">Edit Employee Details</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <form id="editEmpForm">
                <!-- Modal body -->
                <div class="modal-body">

                    <input type="hidden" name="_id" id="_id">

                    <div class="form-row">
                        <div class="form-group col">
                            <label for="FirstName">Test Name: </label>
                            <input type="text" class="form-control" name="EditTestName" id="EditTestName"
                                   placeholder="Enter Test Name">
                        </div>

                        <div class="form-group col">
                            <label for="NoOfQuestions"> No. Of Questions: </label>
                            <input type="number" class="form-control" name="EditNoOfQuestions" id="EditNoOfQuestions"
                                   placeholder="Enter No. Of Questions">
                        </div>
                    </div>

                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="submit" class="btn btn-success" id="editEmployee">Edit Employee</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
